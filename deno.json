{
  "importMap": "import_map.json",
  "name": "@fluentci/buck",
  "version": "0.1.4",
  "exports": "./mod.ts",
  "tasks": {
    "esm:add": "deno run -A https://esm.sh/v128 add",
    "esm:update": "deno run -A https://esm.sh/v128 update",
    "esm:remove": "deno run -A https://esm.sh/v128 remove"
  },
  "fmt": {
    "exclude": [
      "example/",
      "example-go",
      ".fluentci/"
    ]
  },
  "lint": {
    "exclude": [
      "example/",
      "example-go",
      ".fluentci/"
    ]
  },
  "test": {
    "exclude": [
      "example/",
      "example-go",
      ".fluentci/"
    ]
  }
}